<resources>

    <style name="ny_loading_dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>

    <style name="ny_screen_bottombar_popupwindow_anim">
        <item name="android:windowEnterAnimation">@anim/ny_popupwindow_menu_bottombar_in</item>
        <item name="android:windowExitAnimation">@anim/ny_popupwindow_menu_bottombar_out</item>
    </style>

</resources>